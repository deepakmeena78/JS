<html>

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.6.2/dist/css/bootstrap.min.css">
    <script src="https://cdn.jsdelivr.net/npm/jquery@3.7.1/dist/jquery.slim.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.1/dist/umd/popper.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.6.2/dist/js/bootstrap.bundle.min.js"></script>
</head>

<body style="background-color:rgb(132, 207, 182); color: white;">
    <div class="container d-flex justify-content-center align-items-center" style="height:80vh">
        <div class="" style="width:40%; height:auto; box-shadow: 10px 10px 10px grey;">
            <h5 class="bg-secondary p-2 text-center text-white">Data</h5>
            <form class="p-2" method="post" action="/task/whoupdate">
                <input type="hidden" name="imp" value="<%= data.id %>">

                <select class="form-control" name="name"
                    style="background-color: transparent; color: black; box-shadow: 5px 5px 10px gray;">
                    <option style="color: black;" disabled selected>Select Name</option>
                    <% for (let unit of data.labelData) { %>
                        <option value="<%= unit.name %>">
                            <%= unit.name %>
                        </option>
                        <% } %>
                </select>
                <div class="form-group d-flex justify-content-center">
                    <button style="box-shadow: 5px 5px 5px rgb(22, 100, 124);" type="submit"
                        class="btn btn-primary m-1">Done</button>
                </div>
            </form>
        </div>
    </div>
</body>

</html>